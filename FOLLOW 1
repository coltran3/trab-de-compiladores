FOLLOW(S) = { $}
FOLLOW(PROGRAMA) = FOLLOW(S) U FOLLOW(PROGRAMA') U { ACABOU} U FIRST(X')
FOLLOW(PROGRAMA’) = FOLLOW(PROGRAMA)
FOLLOW(COMANDO) = FIRST(PROGRAMA') U FOLLOW(PROGRAMA')
FOLLOW(STMT) = { ;}
FOLLOW(I) = FOLLOW(STMT)
FOLLOW(T) = FOLLOW(STMT)
FOLLOW(X) = FOLLOW(COMANDO)
FOLLOW(X') = FOLLOW(X)
FOLLOW(E) = FOLLOW(COMANDO)
FOLLOW(IDENTIFICADOR) = { =, ;} U FIRST(ARITMÉTICA_LÓGICA) U FOLLOW(ARITMÉTICA_LÓGICA)
FOLLOW(EXPRESSÃO) = { ;, )} U FIRST(ARITMÉTICA_LÓGICA) U FOLLOW(ARITMÉTICA_LÓGICA) U FIRST(EXPRESSÃO_ARITIMÉTICA') U FIRST(EXPRESSÃO_LÓGICA')
FOLLOW(ARITMÉTICA_LÓGICA) = FOLLOW(EXPRESSÃO)
FOLLOW(EXPRESSÃO_ARITIMÉTICA') = FIRST(EXPRESSÃO_LÓGICA') U FOLLOW(EXPRESSÃO_LÓGICA') 
FOLLOW(EXPRESSÃO_LÓGICA') = FOLLOW(ARITMÉTICA_LÓGICA)
FOLLOW(TIPO) = FIRST(IDENTIFICADOR)
FOLLOW(VALOR) = FIRST(ARITMÉTICA_LÓGICA) U FOLLOW(ARITMÉTICA_LÓGICA)
FOLLOW(SINAL) = FIRST(NUMERO)
FOLLOW(N) = FIRST(CASA_DECIMAL) U FOLLOW(CASA_DECIMAL)
FOLLOW(CASA_DECIMAL) = FOLLOW(VALOR)
FOLLOW(LETRA_OU_NUMERO) = FOLLOW(IDENTIFICADOR)
FOLLOW(E_OU) = FIRST(EXPRESSÃO)
FOLLOW(NEGACÃO) = FIRST(EXPRESSÃO)
FOLLOW(OPA) = FIRST(EXPRESSÃO)
FOLLOW(LETRA) = FIRST(LETRA_OU_NUMERO) U FOLLOW(LETRA_OU_NUMERO) 
FOLLOW(NUMERO) = FIRST(N) U FOLLOW(N) U FIRST(LETRA_OU_NUMERO) U FOLLOW(LETRA_OU_NUMERO)


